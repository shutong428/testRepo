<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- 定义远程仓库地址 -->
    <remote  name="github"
             fetch="https://github.com/shutong428/" />

    <!-- 默认分支和远程 -->
    <default revision="master"
             remote="github"
             sync-j="4" />

    <!-- 第一个项目 -->
    <project name="test1"
             path="test1"
             revision="master" />

    <!-- 第二个项目 -->
    <project name="test2"
             path="test2"
             revision="master" />
</manifest>
