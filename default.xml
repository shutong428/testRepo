<?xml version="1.0" encoding="UTF-8"?>
<manifest>
    <!-- 定义远程仓库地址 -->
    <remote  name="gitlab"
             fetch="ssh://git@gitlab.in.leap-io.com/sw-dev/" />

    <!-- 默认分支和远程 -->
    <default revision="master"
             remote="gitlab"
             sync-j="4" />

    <!-- 第一个项目 -->
    <project name="leapcli"
             path="leapcli"
             revision="ir_raid_refactor" />

    <!-- 第二个项目 -->
    <project name="storelib_cpp"
             path="storelib_cpp"
             revision="ir_raid_refactor" />
</manifest>
